#header
  text-shadow 0 0 1px #fff
  margin 50px auto
  div 
    margin-right 86px
  a
    color #eee
    &:hover
      color color-link
      text-decoration none
    
  h1
    font-weight normal
    font-size 2.5em
    line-height 1

  h2
    font-weight normal
    font-size 1.0em
    line-height 1
    margin-top 10px
  
  #main-nav
    font-family font-title
    font-size: 18px
    float left
    line-height 2.5em
    ul
      list-style none
      li
        float left
        margin-left 30px