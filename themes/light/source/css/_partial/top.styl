#r_top
  position fixed
  right 10px
  height 150px
  li
    list-style none
    display block
    width 30px
    height 30px
    border 1px solid #888
    a
      display block
      height 100%
      width 100%
  .top
    background #333333 url("../fancybox/top.png") no-repeat
  .bottom
    background #333333 url("../fancybox/bottom.png") no-repeat
  .close
    background #333333 url("../fancybox/close.png") no-repeat